{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "esnext",
        "target": "es5",
        "lib": [ "es7","dom" ],
        "noImplicitAny": false,
        "sourceMap": false,
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "noEmitOnError": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"],
        },
    },
    "exclude": [
        "node_modules"
    ],
    "typeRoots": [
      "node_modules/@types",
    ]
}
